{
    "openapi": "3.0.0",
    "info": {
        "title": "ExpendManager API",
        "description": "Official documentation of pos sistem AxiomaPos",
        "version": "0.1"
    },
    "servers": [
        {
            "url": "http://127.0.0.1:8000/api/v0.1"
        }
    ],
    "paths": {
        "/projects": {
            "get": {
                "tags": [
                    "Projects"
                ],
                "summary": "All projects for the Expend manager with pagination and filtering",
                "operationId": "8bef29199553f828dc7ed4def4908e8b",
                "parameters": [
                    {
                        "name": "id",
                        "in": "query",
                        "description": "Filter by project ID",
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "search",
                        "in": "query",
                        "description": "Search term to filter projects",
                        "schema": {
                            "type": "string",
                            "example": "project to save money"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "All projects with pagination and filters",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "content": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "message": {
                                                        "type": "string",
                                                        "example": "The list of projects"
                                                    },
                                                    "data": {
                                                        "type": "array",
                                                        "items": {
                                                            "properties": {
                                                                "id": {
                                                                    "type": "integer",
                                                                    "example": 2
                                                                },
                                                                "name": {
                                                                    "type": "string",
                                                                    "example": "Andrew Demner"
                                                                },
                                                                "description": {
                                                                    "type": "string",
                                                                    "example": "Super important wallet description"
                                                                },
                                                                "state": {
                                                                    "type": "string",
                                                                    "example": "1984-09-09 00:00:00"
                                                                },
                                                                "final_date": {
                                                                    "type": "string",
                                                                    "example": "1984-09-09 00:00:00"
                                                                },
                                                                "wallet": {
                                                                    "properties": {
                                                                        "id": {
                                                                            "type": "integer",
                                                                            "example": 2
                                                                        },
                                                                        "name": {
                                                                            "type": "string",
                                                                            "example": "lunch wallet"
                                                                        },
                                                                        "description": {
                                                                            "type": "string",
                                                                            "example": "Principal wallet"
                                                                        },
                                                                        "origin": {
                                                                            "type": "string",
                                                                            "example": "entry principal"
                                                                        },
                                                                        "quantity": {
                                                                            "type": "integer",
                                                                            "example": 18000000
                                                                        }
                                                                    },
                                                                    "type": "object"
                                                                }
                                                            },
                                                            "type": "object"
                                                        }
                                                    },
                                                    "current_page": {
                                                        "type": "integer",
                                                        "example": 1
                                                    },
                                                    "per_page": {
                                                        "type": "integer",
                                                        "example": 10
                                                    },
                                                    "total": {
                                                        "type": "integer",
                                                        "example": 50
                                                    },
                                                    "next_page_url": {
                                                        "type": "string",
                                                        "example": "http://api.com/projects?page=2"
                                                    },
                                                    "prev_page_url": {
                                                        "type": "string",
                                                        "example": null
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "Projects"
                ],
                "summary": "Create projects",
                "operationId": "0e41c95a14992b67baca3e5e8ea751c7",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "description",
                                    "state"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Sale enterprise"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Sale's company"
                                    },
                                    "state": {
                                        "type": "string",
                                        "example": "On progress"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Creation of the project",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Project created successfully"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/projects/{id}": {
            "put": {
                "tags": [
                    "Projects"
                ],
                "summary": "Update data from a project",
                "operationId": "ba0a27d35877ce5f398a02e79a791b2d",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 3
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "description",
                                    "state"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Sale enterprise"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Sale's company"
                                    },
                                    "state": {
                                        "type": "string",
                                        "example": "On progress"
                                    },
                                    "start_date": {
                                        "type": "string",
                                        "format": "date",
                                        "example": "1992-02-17 00:00:00"
                                    },
                                    "final_date": {
                                        "type": "string",
                                        "format": "date",
                                        "example": "final_date"
                                    },
                                    "user_id": {
                                        "type": "integer",
                                        "example": 3
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Updating the project data",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "The project was updated correctly"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "Projects"
                ],
                "summary": "Delete any project by the id",
                "operationId": "a01d1a3ac6a7ae4b3ce6a90ebb92bcf2",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Delete",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": "2"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "The project was deleted correctly"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "description": "put in here your token for try",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
